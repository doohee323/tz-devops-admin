{
  "name": "tz-devops-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run sentry:sourcemaps",
    "lint": "vue-cli-service lint",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org argear --project tz-devops-admin ./dist && sentry-cli --url https://sentry.topzone-k8s.argear.io/ sourcemaps upload --org argear --project tz-devops-admin --release `sentry-cli releases propose-version` ./dist"
  },
  "dependencies": {
    "@sentry/cli": "^2.36.6",
    "@sentry/tracing": "^7.114.0",
    "@sentry/vue": "^8.33.1",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "core-js": "^3.8.3",
    "datatables.net-bs4": "^1.13.1",
    "datatables.net-responsive-bs4": "^2.4.0",
    "datatables.net-select-bs4": "^1.5.0",
    "flatpickr": "^4.6.13",
    "jquery": "^3.6.3",
    "vue": "^2.6.14",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@sentry/webpack-plugin": "^2.22.5",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
